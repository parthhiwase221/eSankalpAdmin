
@{
    ViewBag.Title = "Index";
}

<script src="~/Scripts/jquery-3.7.0.js"></script>
<script src="~/Content/JavaScriptREG.js"></script>

<link rel="stylesheet" href="~/plugins/fontawesome-free/css/all.min.css">
<link rel="stylesheet" href="~/dist/css/adminlte.min.css">




<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">

            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active"> Tables</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Registration List</h3>

                        <div class="card-tools">
                            <div class="input-group input-group-sm" style="width: 150px;">
                                <input type="text" name="table_search" class="form-control float-right" placeholder="Search">

                                <div class="input-group-append">
                                    <button type="submit" class="btn btn-default">
                                        <i class="fas fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body table-responsive p-0" style="height: 300px;">
                        <table id="Reg" class="table table-head-fixed text-nowrap">
                            <thead>
                                <tr>
                                    <th> RegId</th>
                                    <th> Name</th>
                                    <th> MobileNo</th>
                                    <th>  Email </th>
                                    <th>Education </th>
                                    <th> Percentage</th>
                                    <th>  Service</th>
                                    <th> Technology</th>
                                    <th>  Office </th>
                                    <th>College </th>
                                    <th>Address </th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
        </div>
    </div>
</section>



<div class="modal fade" id="GetdetailsModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Details </h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <form>
                    <div class="modal-body">
                        <input type="hidden" id="hdnRegId">
                        <label class="form-label">Full Name</label>
                        <input type="text" id="txtName" class="form-control" name="name" required placeholder="Enter Your Name">

                        <label class="form-label">Mobile No</label>
                        <input type="text" id="txtMobileNo" class="form-control" name="MobileNo" required placeholder="Enter Your mobile no">

                        <label class="form-label">Email</label>
                        <input type="email" id="txtEmail" class="form-control" name="Email" required placeholder="Enter Your Email">


                        <label class="form-label">Education</label>
                        <input type="text" id="txtEducation" class="form-control" name="Education" required placeholder="Enter Your Education">


                        <label class="form-label">Percentage</label>
                        <input type="text" id="txtPercentage" class="form-control" name="Percentage" required placeholder="Enter Your Percentage">


                        <label class="form-label">Service</label>
                        <input type="text" id="txtService" class="form-control" name="Service" required placeholder="Enter Your services">


                        <label class="form-label">Technology</label>
                        <input type="text" id="txtTechnology" class="form-control" name="Technology" required placeholder="Enter Your technology">

                        <label class="form-label">Office</label>
                        <input type="text" id="txtOffice" class="form-control" name="Office" required placeholder="Enter Your office">


                        <label class="form-label">College</label>
                        <input type="text" id="txtCollege" class="form-control" name="College" required placeholder="Enter Your College">

                        <label class="form-label">Address</label>
                        <input type="text" id="txtAddress" class="form-control" name="Address" required placeholder="Enter Your Address">


                    </div>
                </form>
                <div class="modal-footer">
                    <button type="submit" onclick="SaveRegistration()" class="btn btn-success">save</button>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>




<script src="~/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/dist/js/adminlte.min.js"></script>
<script src="~/dist/js/demo.js"></script>